<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<beans profile="prod">
		<bean name="propertiesService" class="egovframework.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
			<property name="properties">
				<map>
					<entry key="systemNm" value="운영"/>

					<!-- 시스템별 환경 설정정보 -->
					<entry key="dirPartition" value="/"/>
					<entry key="uploadFile" value="/data/KVA_Project/upload/"/>
				</map>
			</property>
		</bean>
	</beans>

	<beans profile="dev">
		<bean name="propertiesService" class="egovframework.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
			<property name="properties">
				<map>
					<entry key="systemNm" value="개발"/>

					<!-- 시스템별 환경 설정정보 -->
					<entry key="dirPartition" value="/"/>
					<entry key="uploadFile" value="/home/drugfree/app/webapp/NCMS/uploadfile/"/>
				</map>
			</property>
		</bean>
	</beans>

	<beans profile="loacl">
		<bean name="propertiesService" class="egovframework.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
			<property name="properties">
				<map>
					<entry key="systemNm" value="로컬"/>

					<!-- 시스템별 환경 설정정보 -->
					<entry key="dirPartition" value="\"/>
					<entry key="uploadFile" value="D:\git\NCMS\src\main\webapp\uploadfile\"/>
				</map>
			</property>
		</bean>
	</beans>
</beans>